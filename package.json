        "eslint": "^8.22.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.2.1",
        "pixi.js": "^6.5.1",
        "prettier": "^2.7.1",
        "typescript": "^4.6.4",
        "vite": "^3.0.7",
        "vitest": "^0.21.1"
    },
    "dependencies": {
        "@pixi/sound": "^4.3.0"
